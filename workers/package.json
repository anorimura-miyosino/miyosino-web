{
  "name": "miyosino-api-worker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "dev:contact": "wrangler dev --config wrangler.contact.toml",
    "deploy:contact": "wrangler deploy --config wrangler.contact.toml",
    "dev:contents": "wrangler dev --config wrangler.contents.toml",
    "deploy:contents": "wrangler deploy --config wrangler.contents.toml",
    "dev:auth": "wrangler dev --config wrangler.auth.toml",
    "deploy:auth": "wrangler deploy --config wrangler.auth.toml",
    "dev:announcements": "wrangler dev --config wrangler.announcements.toml",
    "deploy:announcements": "wrangler deploy --config wrangler.announcements.toml",
    "dev:circulars": "wrangler dev --config wrangler.circulars.toml",
    "deploy:circulars": "wrangler deploy --config wrangler.circulars.toml",
    "dev:minutes": "wrangler dev --config wrangler.minutes.toml",
    "deploy:minutes": "wrangler deploy --config wrangler.minutes.toml",
    "set-jwt-secret": "bash scripts/set-jwt-secret.sh",
    "generate-jwt-secret": "bash scripts/generate-jwt-secret.sh",
    "build": "tsc",
    "build:watch": "tsc --watch"
  },
  "devDependencies": {
    "wrangler": "^4.47.0",
    "@cloudflare/workers-types": "^4.20241106.0",
    "typescript": "^5"
  }
}